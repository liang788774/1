<script type="text/javascript">
// 监控guess域名下url请求时间
(function(){
    var reported = {};
    var report = window.__script_loaded = function(src, type, data) {
        
        if(reported[src]['loaded']){
            return;
        }
        reported[src]['loaded'] = true;

        var timeEnd = new Date().getTime();
        var timeLast = timeEnd - reported[src]['timeStart'];
        
        var obj = {
            time: timeLast,
            data: data
        };
        hao360.iguessTrack('loaded_empty_' + type, obj);
    };
    function load_script(src, type) {

        // // 加载脚本
        reported[src] = {};
        
        reported[src]['timeStart'] = new Date().getTime();
        var onerror = '__script_loaded(src, \'' + type + '\', \'error\')';
        hao360.iguessTrack('start_empty_' + type);
        hao360.docWrite('<script type="text/javascript" onreadystatechange="if(this.readyState===\'loaded\' || this.readyState===\'complete\')__script_loaded(src, \'' + type + '\', \'loaded\')" onload="__script_loaded(src, \'' + type + '\', \'loaded\')" src="' + src + '" onerror="' + onerror + '"><\/script>');

    }


    if(SERVER_DATA.guess_track && hao360.isHttps()){
        load_script(STATIC_CONFIG.guess.monitor, 'gif');
        load_script('https://s2.ssl.qhres.com/!ac8b00fc/haha.js', 'cdn');
    }
})();

if (MINI_APPS_CONFIG.length) {
    qboot.load('hotsiteMiniApps');
}
</script>
